{
  "name": "vue-project",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build --emptyOutDir",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "ayoba-microapp-api": "^2.0.0",
    "bootstrap": "^5.3.2",
    "nanoid": "^5.0.5",
    "vue": "^3.4.15",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^9.9.1",
    "vue3-notification": "^0.0.3",
    "vue3-paystack": "^0.0.5",
    "vue3-barcode-qrcode-reader": "^1.0.16"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-singlefile": "^1.0.0",
    "vue-tsc": "^1.8.27"
  }
}
